//Constantes
int n=100,m=100;
real dt=0.1;
real TF=2.;

//DÃ©finition du maillage
mesh Th=square(n,m,[1*x,1*y]);
//plot(Th, ps="truc.ps");

//Espace d'approximation
fespace Vh(Th,P1);

//Fonction source

Vh uh,vh, uhp1, f;

f=x^5*y^3+y+x^3;
uh=0;


solve P(uhp1,vh)=int2d(Th)(dx(uhp1)*dx(vh)+dy(uhp1)*dy(vh))
	      - int2d(Th)(f*vh)
	      + on(1,2,3,4,uhp1=0);

plot(uh, fill=true, dim=3, value=true);

